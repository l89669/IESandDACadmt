<Window x:Class="IESandDACadmt.View.WpfEventTypeSelection"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IESandDACadmt.View"
        mc:Ignorable="d"
        Title="WpfEventTypeSelection" Height="300" Width="250">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="10*" />
            <RowDefinition Height="1*" />
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" Text="Select Event Types To Delete:" HorizontalAlignment="Center" />

        <StackPanel Grid.Row="2" Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Name="OK" VerticalAlignment="Center" HorizontalAlignment="Center" Content="OK" Padding="25,0,25,0" Click="OK_Click" Grid.Row="2" Grid.Column="1"/>
            <TextBlock Text="   "/>
            <Button Name="CANCEL" VerticalAlignment="Center" HorizontalAlignment="Center" Content="CANCEL" Padding="12,0,12,0" Click="CANCEL_Click" Grid.Row="2" Grid.Column="1"/>
        </StackPanel>
    </Grid>
</Window>
